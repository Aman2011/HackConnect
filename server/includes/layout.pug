doctype html
html
    head
        title HackConnect
        //link(href="/favicon.ico", rel="shortcut icon", type="image/x-icon")
        include head
        base(href="/")
    body.no-padding(ng-app='app' ng-controller='MainCtrl')
        include currentUser
        include navbar
        .content(ng-class="isInbox()?'display-flex':''")
            block main-content
        footer.text-center
            p.no-margin &copy; 2016 HackConnect
        include scripts
        block scripts
        script(type="text/javascript", src="/socket.io/socket.io.js")
        script.
            var socket = io.connect("http://localhost:7700");
            socket.emit('log in', {
                id: window.bootstrappedUserObject._id
            })
        script(type="text/javascript", src="/app/main/app.js")
        script(type="text/javascript", src="/app/main/controllers/create-profile.js")
        script(type="text/javascript", src="/app/main/controllers/profile.js")
        script(type="text/javascript", src="/app/main/controllers/users.js")
        script(type="text/javascript", src="/app/main/controllers/user.js")
        script(type="text/javascript", src="/app/main/controllers/navbar.js")
        script(type="text/javascript", src="/app/main/controllers/inbox.js")
        script(type="text/javascript", src="/app/main/controllers/hackathons.js")
        script(type="text/javascript", src="/app/main/controllers/hackathon.js")
        script(type="text/javascript", src="/app/main/controllers/main.js")
        script(type="text/javascript", src="/app/main/services/hackathonService.js")
        script(type="text/javascript", src="/app/main/services/hackathonInitialData.js")
        script(type="text/javascript", src="/app/main/services/socket.js")
        script(type="text/javascript", src="/app/main/services/notifier.js")
        script(type="text/javascript", src="/app/main/services/userService.js")
        script(type="text/javascript", src="/app/main/services/userInitialData.js")
        script(type="text/javascript", src="/app/main/services/dataService.js")







